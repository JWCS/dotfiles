# Found below
[include]
  path = ~/.gitconfig_important
[push]
	default = simple
[core]
	excludesfile = ~/.gitignore_global
[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
[alias]
    lg = !"git lg1"
    lga = !"git lga1"
    lgs = !"git lgs1"
    lgas = !"git lgas1"
    lga1 = !"git lg1 --all"
    lga2 = !"git lg2 --all"
    lga3 = !"git lg3 --all"
    lgs1 = !"git lg1 --simplify-by-decoration"
    lgs2 = !"git lg2 --simplify-by-decoration"
    lgs3 = !"git lg3 --simplify-by-decoration"
    lgas1 = !"git lg1 --all --simplify-by-decoration"
    lgas2 = !"git lg2 --all --simplify-by-decoration"
    lgas3 = !"git lg3 --all --simplify-by-decoration"
    # Can make the colors bigger/bolder by adding 'bold', ex "%C(bold blue)%h%C(reset) ..."; likewise, can make 'dim'
    lg1 = log --graph --abbrev-commit --decorate --format=format:'%C(blue)%h%C(reset) - %C(green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(auto)%d%C(reset)'
    lg2 = log --graph --abbrev-commit --decorate --format=format:'%C(blue)%h%C(reset) - %C(cyan)%aD%C(reset) %C(green)(%ar)%C(reset)%C(auto)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)'
    lg3 = log --graph --abbrev-commit --decorate --format=format:'%C(blue)%h%C(reset) - %C(cyan)%aD%C(reset) %C(green)(%ar)%C(reset) %C(cyan)(committed: %cD)%C(reset) %C(auto)%d%C(reset)%n''          %C(white)%s%C(reset)%n''          %C(dim white)- %an <%ae> %C(reset) %C(dim white)(committer: %cn <%ce>)%C(reset)'
[rerere]
	enabled = true
	autoupdate = true
